{
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(x)?$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "recurse": true,
      "destination": "docs/"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }
  